{
    "name": "pdgp/server",
    "description": "Product Display Generator Project - Server",
    "type": "project",
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.10",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}

